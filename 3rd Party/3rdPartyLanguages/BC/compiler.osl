code = "input bc code here".ask

code = code.regex("/(?<=\()[^)]+(?=\))|[^ \n()%;]+|[()]|%[^%]*%|;/gm")
loop code.len (
  count += 1
  cur = code.[count]
  last = cur.len
  if cur.[1] == "%" and ( cur.[last] == "%" ) (
    cur = ""
  )
)

cons = "> Successfully loaded console. (v0.1)"
window "show"
loggingliney = -320
logginglinex = 160
mainloop:
c #fff
goto -340 180
text "BetterCode Console" 10
goto loggingliney logginglinex
text cons 8
import "win-buttons"
