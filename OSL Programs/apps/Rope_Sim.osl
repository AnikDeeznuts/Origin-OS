x = 31.new("array")
y = 31.new("array")

log x
log y

mainloop:
x.[1] = mouse_x
y.[1] = mouse_y

i = 1
loop x.len - 1 (
  i += 1
  i2 = i - 1
  x.[i] += x.[i2] - x.[i] / 5
  y.[i] += y.[i2] - y.[i] / 5
)

goto x.[1] y.[1]
c #fff
pen "size" 5
pen "down"
i = 1
loop x.len - 1 (
  i += 1
  goto x.[i] y.[i]
)
pen "up"

import "win-buttons"
